  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/repository/**"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  }
}
